// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model questions {
  id      Int      @id @default(autoincrement())
  question String
  votes Int
  user Int
  talkId Int
  ok Boolean
}
model features{
  id      Int      @id @default(autoincrement())
  questions Boolean
  questionsList Boolean
  stars Boolean
  location String
}
model stars{
  id      Int      @id @default(autoincrement())
  user Int
  stars Int
  talkId Int
  locationId Int
}
model users{
  id      Int      @id @default(autoincrement())
  user String
  email Int
  voteCount Int
}
